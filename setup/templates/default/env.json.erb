{
  <% i = 0 %>
  <% @definitions.each_pair do |key,val| -%>
    <% if val.is_a?(Numeric) -%>
  "<%= key %>":<%= val %><%= ',' if i < (@definitions.size - 1) %>
    <% elsif val.is_a?(Hash) || val.is_a?(Array) -%>
  "<%= key %>":<%= val.to_json %><%= ',' if i < (@definitions.size - 1) %>
    <% else -%>
  "<%= key %>":"<%= val %>"<%= ',' if i < (@definitions.size - 1) %>
    <% end -%>
    <% i += 1 %>
  <% end -%>
}